<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>nicholsab</Authors>
    <Description>.NET Standard 2.0 compatible JBig2 to System.Drawing.Image converter.  Based off of https://www.nuget.org/packages/JBig2Decoder.NETCore/</Description>
    <Copyright>nicholsab 2020</Copyright>
    <PackageReleaseNotes>A .NET Standard 2.0 version of JBig2Decoder.NET https://www.nuget.org/packages/JBig2Decoder.NET/.  Which is a .Net Port of the JPedal JBig2Decoder Implementation.</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/nicholsab/JBig2Decoder.NETStandard</RepositoryUrl>
    <PackageProjectUrl>https://github.com/nicholsab/JBig2Decoder.NETStandard</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>1.3.0.0</AssemblyVersion>
    <FileVersion>1.3.0.0</FileVersion>
    <Version>1.3.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="4.7.0" />
  </ItemGroup>

</Project>
